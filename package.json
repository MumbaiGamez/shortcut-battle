{
  "name": "shortcut-battle",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "npm run build:ssr:dev && npm-run-all --parallel watch:client start:dev",
    "prod": "npm run build:ssr:prod && npm run build:client && npm run start:prod",
    "start:dev": "NODE_ENV=development TS_NODE_PROJECT=server.tsconfig.json ts-node-dev -r tsconfig-paths/register --watch server,lib --transpile-only --respawn server",
    "start:prod": "NODE_ENV=production TS_NODE_PROJECT=server.tsconfig.json ts-node -r tsconfig-paths/register server",
    "watch:client": "NODE_ENV=development webpack --watch --config bundler/config/client.config.ts",
    "build:client": "NODE_ENV=production webpack --config bundler/config/client.config.ts",
    "build:ssr:dev": "NODE_ENV=development webpack --config bundler/config/ssr.config.ts",
    "build:ssr:prod": "NODE_ENV=production webpack --config bundler/config/ssr.config.ts",
    "docker:up": "docker-compose up --remove-orphans --build -d",
    "docker:down": "docker-compose down",
    "docker:ps": "docker-compose ps --all",
    "docker:stop": "docker stop $(docker ps -q)",
    "docker:rm": "docker rm $(docker ps -a -q)",
    "docker:clear": "docker volume rm $(docker volume ls -q)",
    "lint": "npm-run-all lint:*",
    "lint:ts": "eslint --ext=.ts,.tsx src --fix",
    "lint:css": "stylelint src/**/**/*.css --fix",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MumbaiGamez/shortcut-battle.git"
  },
