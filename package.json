{
  "name": "shortcut-battle",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node dist/server/index.js",
    "start:dev": "NODE_ENV=development node dist/server/index.js",
    "clean": "rimraf dist",
    "build:client": "webpack --config bundler/config/client.config.ts",
    "watch:client": "NODE_ENV=development webpack --watch --config bundler/config/client.config.ts",
    "build:ssr": "webpack --config bundler/config/ssr.config.ts",
    "watch:ssr": "NODE_ENV=development webpack --watch --config bundler/config/ssr.config.ts",
    "build:server": "tsc --project server && tsc-alias -p server/tsconfig.json",
    "build": "NODE_ENV=production npm-run-all clean build:*",
    "dev": "npm-run-all clean build:server && npm-run-all --parallel --print-name watch:client watch:ssr start:dev",
    "lint": "npm-run-all lint:*",
    "lint:ts": "eslint --ext=.ts,.tsx src --fix",
    "lint:css": "stylelint src/**/**/*.css --fix",
    "docker:build": "docker build -t shortcut-battle .",
    "docker:start": "docker run -p 3000:3000 -d shortcut-battle",
    "docker:stop": "docker stop $(docker ps -q --filter ancestor=shortcut-battle)",
    "heroku:release": "heroku container:login && heroku container:push web -a shortcut-battle && heroku container:release web -a shortcut-battle",
    "prepare": "husky install",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "tsc": "node ./node_modules/.bin/tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MumbaiGamez/shortcut-battle.git"
  },
  "keywords": [],
  "author": "MumbaiGamez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MumbaiGamez/shortcut-battle/issues"
  },
  "homepage": "https://github.com/MumbaiGamez/shortcut-battle#readme",
